{
  "name": "og-image",
  "alias": ["og-image.now.sh"],
  "version": 2,
  "regions": ["iad1", "sfo1"],
  "public": false,
  "builds": [
    {
      "src": "src/card.ts",
      "use": "@now/node",
      "config": { "maxLambdaSize": "36mb" }
    }
  ],
  "routes": [{ "src": "/og.jpg", "dest": "/src/card.ts" }]
}
